{
  "name": "Number",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "status": {
      "type": "string",
      "required": true,
      "default": "in line"
    }
  },
  "validations": [],
  "relations": {
    "doctor": {
      "type": "belongsTo",
      "model": "AppUser",
      "foreignKey": "doctorId"
    },
    "patient": {
      "type": "belongsTo",
      "model": "AppUser",
      "foreignKey": "patientId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
